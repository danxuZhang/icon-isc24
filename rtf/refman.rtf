00597   {\cf17 auto} loop3_duration = std::chrono::duration_cast<std::chrono::milliseconds>(loop3_end - loop3_start);\par
00598   std::cout << {\cf22 "time for loop three: "} << loop3_duration.count() << {\cf22 "ms"} << std::endl;\par
00599 \par
00600   {\cf19 for} ({\cf18 size_t} i = 0; i < nvec; ++i) \{\par
00601     free(vt[i]);\par
00602     free(kmin[i]);\par
00603   \}\par
