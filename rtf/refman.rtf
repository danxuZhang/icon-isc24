00015 {\cf18 void} utils_muphys::calc_dz(array_1d_t<real_t> &z, array_1d_t<real_t> &dz,\par
00016                            {\cf18 size_t} &ncells, {\cf18 size_t} &nlev) \{\par
00017   dz.resize(ncells * nlev);\par
00018   array_2d_t<real_t> zh(nlev + 1, array_1d_t<real_t>(ncells));\par
00019 \par
00020   {\cf19 for} ({\cf18 size_t} i = 0; i < ncells; i++) \{\par
00021     zh[nlev][i] = ({\cf17 static_cast<}real_t{\cf17 >}(3.0) * z[i + (nlev - 1) * (ncells)] -\par
00022                    z[i + (nlev - 2) * (ncells)]) *\par
00023                   {\cf17 static_cast<}real_t{\cf17 >}(0.5);\par
00024   \}\par
00025 \par
00026   {\cf20 // The loop is intentionally i<nlev; since we are using an unsigned integer}\par
00027   {\cf20 // data type, when i reaches 0, and you try to decrement further, (to -1), it}\par
00028   {\cf20 // wraps to the maximum value representable by size_t.}\par
00029   {\cf19 for} ({\cf18 size_t} i = nlev - 1; i < nlev; --i) \{\par
00030     {\cf19 for} ({\cf18 size_t} j = 0; j < ncells; j++) \{\par
00031       zh[i][j] = {\cf17 static_cast<}real_t{\cf17 >}(2.0) * z[j + (i * ncells)] - zh[i + 1][j];\par
00032       dz[i * ncells + j] = -zh[i + 1][j] + zh[i][j];\par
00033     \}\par
00034   \}\par
00035 \}\par
}
