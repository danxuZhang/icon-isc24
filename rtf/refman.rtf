00083   {\cf17 auto} duration = std::chrono::duration_cast<std::chrono::milliseconds>(\par
00084       end_time - start_time);\par
00085 \par
00086   std::cout << {\cf22 "time taken : "} << duration.count() << {\cf22 " milliseconds"}\par
00087             << std::endl;\par
00088   io_muphys::write_fields(output_file, ncells, nlev, t, qv, qc, qi, qr, qs, qg);\par
00089 \par
