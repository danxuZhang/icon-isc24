00572         \}\par
00573 \par
00574         pflx[oned_vec_index] = q[lqs].p[iv] + q[lqi].p[iv] + q[lqg].p[iv];\par
00575 \par
00576         {\cf20 // updates the flux of energy (eflx) associated with precipitation, }\par
00577         eflx[iv] =\par
00578             dt * (q[lqr].p[iv] * (clw * t[oned_vec_index] -\par
00579                                   cvd * t[kp1 * ivend + iv] - lvc) +\par
00580                   pflx[oned_vec_index] * (ci * t[oned_vec_index] -\par
