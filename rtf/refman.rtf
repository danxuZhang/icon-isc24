00422       sx2x[lqv][lqs] =\par
00423           vapor_x_snow(t[oned_vec_index], p[oned_vec_index],\par
00424                        rho[oned_vec_index], q[lqs].x[oned_vec_index], n_snow,\par
00425                        l_snow, eta, ice_dep, dvsw, dvsi, dvsw0, dt);\par
00426       {\cf20 // ensure that the calculated transformation rates do not fall into physically impossible values}\par
00427       sx2x[lqs][lqv] = -std::fmin(sx2x[lqv][lqs], 0.0);\par
00428       sx2x[lqv][lqs] = std::fmax(sx2x[lqv][lqs], 0.0);\par
00429       sx2x[lqv][lqg] = vapor_x_graupel(\par
00430           t[oned_vec_index], p[oned_vec_index], rho[oned_vec_index],\par
00431           q[lqg].x[oned_vec_index], dvsw, dvsi, dvsw0, dt);\par
00432       \par
00433       {\cf20 // ensure that the calculated transformation rates do not fall into physically impossible values}\par
00434       sx2x[lqg][lqv] = -std::fmin(sx2x[lqv][lqg], 0.0);\par
00435       sx2x[lqv][lqg] = std::fmax(sx2x[lqv][lqg], 0.0);\par
00436       sx2x[lqs][lqr] =\par
00437           snow_to_rain(t[oned_vec_index], p[oned_vec_index],\par
00438                        rho[oned_vec_index], dvsw0, q[lqs].x[oned_vec_index]);\par
00439       sx2x[lqg][lqr] =\par
00440           graupel_to_rain(t[oned_vec_index], p[oned_vec_index],\par
00441                           rho[oned_vec_index], dvsw0, q[lqg].x[oned_vec_index]);\par
00442     \}\par
00443     {\cf20 // After calculating the exchange rates between hydrometeor types, }\par
00444     {\cf20 // the loop updates the concentrations (dqdt)}\par
00445 \par
00446 {\cf21     #pragma acc loop seq}\par
00447     {\cf19 for} ({\cf18 size_t} ix = 0; ix < nx; ix++) \{\par
00448       sink[qx_ind[ix]] = 0.0;\par
00449       {\cf19 if} ((is_sig_present[j]) or (qx_ind[ix] == lqc) or (qx_ind[ix] == lqv) or\par
00450           (qx_ind[ix] == lqr)) \{\par
00451 {\cf21         #pragma acc loop seq}\par
00452         {\cf19 for} ({\cf18 size_t} i = 0; i < nx; i++) \{\par
00453           sink[qx_ind[ix]] = sink[qx_ind[ix]] + sx2x[qx_ind[ix]][i];\par
00454         \}\par
00455         stot = q[qx_ind[ix]].x[oned_vec_index] / dt;\par
00456         {\cf20 // check if total sink exceeds the available amount (stot) of a hydrometeor.}\par
00457         {\cf19 if} ((sink[qx_ind[ix]] > stot) &&\par
00458             (q[qx_ind[ix]].x[oned_vec_index] > qmin)) \{\par
00459           real_t nextSink = 0.0;\par
00460 {\cf21           #pragma acc loop seq}\par
00461           {\cf19 for} ({\cf18 size_t} i = 0; i < nx; i++) \{\par
00462             sx2x[qx_ind[ix]][i] = sx2x[qx_ind[ix]][i] * stot / sink[qx_ind[ix]];\par
00463             nextSink = nextSink + sx2x[qx_ind[ix]][i];\par
00464           \}\par
00465           sink[qx_ind[ix]] = nextSink;\par
00466         \}\par
00467       \}\par
00468     \}\par
00469 \par
00470     {\cf20 // ensures conservation by making sure that increases in one hydrometeor type }\par
00471     {\cf20 // are balanced by decreases in others (e.g., cloud water, if rain is formed by condensation).}\par
00472 {\cf21     #pragma acc loop seq}\par
00473     {\cf19 for} ({\cf18 size_t} ix = 0; ix < nx; ix++) \{\par
00474       sx2x_sum = 0;\par
00475 {\cf21       #pragma acc loop seq}\par
00476       {\cf19 for} ({\cf18 size_t} i = 0; i < nx; i++) \{\par
00477         sx2x_sum = sx2x_sum + sx2x[i][qx_ind[ix]];\par
00478       \}\par
00479       dqdt[qx_ind[ix]] = sx2x_sum - sink[qx_ind[ix]];\par
00480       q[qx_ind[ix]].x[oned_vec_index] = std::fmax(\par
00481           0.0, q[qx_ind[ix]].x[oned_vec_index] + dqdt[qx_ind[ix]] * dt);\par
00482     \}\par
00483 \par
00484     qice = q[lqs].x[oned_vec_index] + q[lqi].x[oned_vec_index] +\par
00485            q[lqg].x[oned_vec_index];\par
00486     qliq = q[lqc].x[oned_vec_index] + q[lqr].x[oned_vec_index];\par
00487     qtot = q[lqv].x[oned_vec_index] + qice + qliq;\par
00488     cv = cvd + (cvv - cvd) * qtot + (clw - cvv) * qliq +\par
00489          (ci - cvv) * qice; {\cf20 // qtot? or qv?}\par
00490     {\cf20 // adjustment of temperature based on the latent heat release or absorption during phase changes}\par
00491     t[oned_vec_index] =\par
00492         t[oned_vec_index] +\par
00493         dt *\par
00494             ((dqdt[lqc] + dqdt[lqr]) * (lvc - (clw - cvv) * t[oned_vec_index]) +\par
00495              (dqdt[lqi] + dqdt[lqs] + dqdt[lqg]) *\par
00496                  (lsc - (ci - cvv) * t[oned_vec_index])) /\par
00497             cv;\par
00498   \}\par
00499   {\cf20 // basically update to the overall atmospheric state at each significant point, }\par
00500   {\cf17 auto} loop2_end = std::chrono::steady_clock::now();\par
00501   {\cf17 auto} loop2_duration = std::chrono::duration_cast<std::chrono::milliseconds>(loop2_end - loop2_start);\par
00502   std::cout << {\cf22 "time for loop two: "} << loop2_duration.count() << {\cf22 "ms"} << std::endl;\par
00503 \par
00504   {\cf18 size_t} kp1;\par
00505   {\cf18 size_t} k_end = (lrain) ? ke : kstart - 1;\par
00506   {\cf17 auto} loop3_start = std::chrono::steady_clock::now();\par
00507 \par
00508 \par
00528 {\cf21   #pragma acc parallel }\par
00529 {\cf21   #pragma acc loop seq }\par
00530   {\cf19 for} ({\cf18 size_t} k = kstart; k < k_end; k++) \{\par
00531 {\cf21     # pragma acc loop private(oned_vec_index, kp1, qliq, qice, e_int, zeta, xrho, vc, update)}\par
00532     {\cf19 for} ({\cf18 size_t} iv = ivstart; iv < ivend; iv++) \{\par
00533       oned_vec_index = k * ivend + iv;\par
00534       {\cf19 if} (k == kstart) \{\par
00535         eflx[iv] = 0.0;\par
00536       \}\par
00537 \par
00538       kp1 = std::min(ke - 1, k + 1);\par
00539       \par
00540       {\cf20 // feedback between precipitation processes and atmospheric conditions}\par
00541       {\cf19 if} (k >= get_min_element(kmin[iv], np)) \{\par
00542         {\cf20 // calculates the liquid and ice water content at each point}\par
00543         qliq = q[lqc].x[oned_vec_index] + q[lqr].x[oned_vec_index];\par
00544         qice = q[lqs].x[oned_vec_index] + q[lqi].x[oned_vec_index] +\par
00545                q[lqg].x[oned_vec_index];\par
00546         {\cf20 // Internal energy (e_int) based on temperature, hydrometeor content, and air density.}\par
00547         e_int =\par
00548             internal_energy(t[oned_vec_index], q[lqv].x[oned_vec_index], qliq,\par
00549                             qice, rho[oned_vec_index], dz[oned_vec_index]) +\par
00550             eflx[iv];\par
00551 \par
00552         zeta = dt / (2.0 * dz[oned_vec_index]);\par
00553         xrho = std::sqrt(rho_00 / rho[oned_vec_index]);\par
00554 \par
00555         \par
00556         {\cf20 // simulate how precipitation and its associated energy }\par
00557         {\cf20 // move vertically through the atmosphere.}\par
00558         {\cf19 for} ({\cf18 size_t} ix = 0; ix < np; ix++) \{\par
00559           {\cf19 if} (k >= kmin[iv][qp_ind[ix]]) \{\par
00560             vc = vel_scale_factor(qp_ind[ix], xrho, rho[oned_vec_index],\par
00561                                   t[oned_vec_index],\par
00562                                   q[qp_ind[ix]].x[oned_vec_index]);\par
00563 \par
00564             {\cf20 // simulates precipitation processes, adjusting the vertical distribution of different hydrometeors}\par
00565             precip(params[qp_ind[ix]], update, zeta, vc, q[qp_ind[ix]].p[iv],\par
00566                    vt[iv][ix], q[qp_ind[ix]].x[oned_vec_index],\par
00567                    q[qp_ind[ix]].x[kp1 * ivend + iv], rho[oned_vec_index]);\par
00568             q[qp_ind[ix]].x[oned_vec_index] = update[0];\par
00569             q[qp_ind[ix]].p[iv] = update[1];\par
