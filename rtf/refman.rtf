00583         qliq = q[lqc].x[oned_vec_index] + q[lqr].x[oned_vec_index];\par
00584         qice = q[lqs].x[oned_vec_index] + q[lqi].x[oned_vec_index] +\par
00585                q[lqg].x[oned_vec_index];\par
