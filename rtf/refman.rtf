00110 {\cf18 void} precip({\cf17 const} real_t (&params)[3], real_t (&precip)[3], real_t zeta,\par
00111             real_t vc, real_t flx, real_t vt, real_t q, real_t q_kp1,\par
00112             real_t rho) \{\par
00113   real_t rho_x, flx_eff, flx_partial;\par
00114   {\cf20 // Calculation of the effective density of precipitation in the cell}\par
00115   rho_x = q * rho;\par
00116   {\cf20 // Effective flux of precipitation into the cell, considering both the density and the initial flux}\par
00117   flx_eff = (rho_x / zeta) + {\cf17 static_cast<}real_t{\cf17 >}(2.0) * flx;\par
00118   {\cf20 // Partial flux of precipitation, adjusted for the fall speed correction factor}\par
00119   flx_partial = rho_x * vc * fall_speed(rho_x, params);\par
00120   flx_partial = std::fmin(flx_partial, flx_eff);\par
00121   {\cf20 // Update the specific mass of hydrometeors in the cell}\par
00122   precip[0] = (zeta * (flx_eff - flx_partial)) /\par
00123               (({\cf17 static_cast<}real_t{\cf17 >}(1.0) + zeta * vt) * rho);\par
00124   {\cf20 // Update the flux of precipitation from the cell above}\par
00125   precip[1] = (precip[0] * rho * vt + flx_partial) * {\cf17 static_cast<}real_t{\cf17 >}(0.5);\par
00126   {\cf20 // Recalculate the density of precipitation, considering the updates}\par
00127   rho_x = (precip[0] + q_kp1) * {\cf17 static_cast<}real_t{\cf17 >}(0.5) * rho;\par
00128   {\cf20 // Update the terminal velocity of precipitation particles}\par
00129   precip[2] = vc * fall_speed(rho_x, params);\par
00130 \}\par
