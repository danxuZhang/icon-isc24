00075   qnc_1 = qnc;\par
00076 \par
00077   {\cf17 auto} start_time = std::chrono::steady_clock::now();\par
00078 \par
00079   graupel(nvec, kend, ivbeg, ivend, kbeg, dt, dz, t, rho, p, qv, qc, qi, qr, qs,\par
