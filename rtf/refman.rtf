00200 {\cf17 static} {\cf18 void} copy_coordinate_variables(NcFile &datafile, NcFile &inputfile,\par
00201                                       array_1d_t<std::string> coordinates) \{\par
00202   {\cf19 for} ({\cf17 auto} &coordinate_name : coordinates) \{\par
00203     {\cf17 auto} coordinate = inputfile.getVar(coordinate_name);\par
00204     {\cf20 /* copy possible new dimensions from input coordinates to the output}\par
00205 {\cf20      * befor adding the related data variables */}\par
00206     {\cf19 for} (NcDim &dim : coordinate.getDims()) \{\par
00207       {\cf17 auto} currentDims = datafile.getDims();\par
00208       {\cf20 /* map.contains() would be better, but requires c++20 */}\par
00209       {\cf19 if} ({\cf17 auto} search = currentDims.find(dim.getName());\par
00210           search == currentDims.end())\par
00211         datafile.addDim(dim.getName(), dim.getSize());\par
00212     \}\par
00213     {\cf17 auto} var = datafile.addVar(coordinate.getName(), coordinate.getType(),\par
00214                                coordinate.getDims());\par
00215 \par
00216     {\cf19 for} ({\cf17 auto} &[attribute_name, attribute] : coordinate.getAtts()) \{\par
00217       std::string dataValues = {\cf22 "default"};\par
00218       attribute.getValues(dataValues);\par
00219       var.putAtt(attribute.getName(), attribute.getType(),\par
00220                  attribute.getAttLength(), dataValues.c_str());\par
00221     \}\par
00222 \par
00223     {\cf20 /* Calculate the total size of the varibale */}\par
00224     {\cf17 auto} dimensions = coordinate.getDims();\par
00225     {\cf17 struct }Prod \{\par
00226       {\cf18 void} operator()(NcDim n) \{ prod *= n.getSize(); \}\par
00227       {\cf18 size_t} prod\{1\};\par
00228     \};\par
00229     {\cf18 size_t} totalSize =\par
00230         std::for_each(dimensions.cbegin(), dimensions.cend(), Prod()).prod;\par
00231 \par
00232     {\cf20 /* Create a one-dimensional vector to store its values */}\par
00233     array_1d_t<real_t> oneDimensionalVariable(totalSize);\par
00234 \par
00235     {\cf20 /* Copy the original values to the output */}\par
00236     coordinate.getVar(oneDimensionalVariable.data());\par
00237     var.putVar(oneDimensionalVariable.data());\par
00238   \}\par
00239 \}\par
